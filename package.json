    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vue": "^3.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@eslint/js": "^9.9.1",
